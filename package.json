{
  "name": "electron-react-hot-ts-seed",
  "version": "1.0.1",
  "description": "React/Typescript bolierplate project for Electron apps using Webpack and hot-reloading",
  "main": "./dist/main.js",
  "scripts": {
    "start": "electron ./dist/main.js",
    "devstart": "cross-env ELECTRON_DEV=dev electron ./dist/main.js",
    "dev": "webpack --config ./config/webpack.config.dev.js",
    "build": "webpack --config ./config/webpack.config.prod.js",
    "server": "yarn run dev && webpack-dev-server --config ./config/webpack.config.server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "webpack4",
    "webpack",
    "react-hot-loader-3",
    "hot-reloading"
  ],
  "author": "Antonio Vizuete",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-hot-loader": "4.1.1",
    "@types/webpack-env": "1.15.2",
    "ajv": "^6.12.3",
    "cross-env": "7.0.2",
    "css-loader": "3.6.0",
    "electron": "9.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "node-sass": "4.14.1",
    "react-hot-loader": "4.12.21",
    "sass-loader": "7.0.0",
    "source-map-loader": "1.0.1",
    "style-loader": "1.2.1",
    "ts-loader": "7.0.5",
    "typescript": "3.9.6",
    "webpack": "4.10.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.1.1"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  }
}
